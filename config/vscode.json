{
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "editor.tabSize": 2,
  "editor.fontFamily": "'JetBrains Mono','Droid Sans Mono', 'monospace'",
  "editor.fontLigatures": true,
  "editor.wordWrap": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "gutterpreview.showImagePreviewOnGutter": false,
  "workbench.iconTheme": "material-icon-theme",
  "editor.guides.bracketPairs": true,
  "explorer.confirmDragAndDrop": false,
  "workbench.colorTheme": "One Dark Pro Flat",
  "rapidapi.terminalLink.enabled": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "screencastMode.fontSize": 20,
  "screencastMode.verticalOffset": 5,
  "editor.minimap.enabled": false,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": "#2980b9",
  "vim.statusBarColors.insert": "#8e44ad",
  "vim.statusBarColors.visual": "#1abc9c",
  "vim.statusBarColors.visualline": "#16a085",
  "vim.statusBarColors.visualblock": "#27ae60",
  "vim.statusBarColors.replace": "#d35400",
  "vim.statusBarColors.commandlineinprogress": "#007ACC",
  "vim.statusBarColors.searchinprogressmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#007ACC",
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "workbench.colorCustomizations": {
    "statusBar.background": "#1abc9c",
    "statusBar.noFolderBackground": "#1abc9c",
    "statusBar.debuggingBackground": "#1abc9c",
    "statusBar.foreground": "#434C5E",
    "statusBar.debuggingForeground": "#434C5E"
  },
  "vim.argumentObjectClosingDelimiters": [")", "]", ">", "}"],
  "vim.argumentObjectOpeningDelimiters": ["(", "[", "<", "{"],
  "html.format.templating": true,
  "files.autoSave": "afterDelay",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["u"],
      "after": [],
      "commands": [
        {
          "command": "undo"
        }
      ]
    },
    {
      "before": ["<C-r>"],
      "after": [],
      "commands": [
        {
          "command": "redo"
        }
      ]
    }
  ]
}
